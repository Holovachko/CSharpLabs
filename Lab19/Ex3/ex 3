using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.IO;

namespace Ex3
{
    [Serializable]
    class Restourant
    {
        public string name, dish, creator, cooking;

        public Restourant(string a , string b , string c , string d)
        {
            this.name = a;
            this.dish = b;
            this.creator = c;
            this.cooking = d;
        }
    }
}

